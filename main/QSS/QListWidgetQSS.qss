QListWidget, QListView, QTreeWidget, QTreeView {
    outline: 0px;
}

QListWidget {
    min-width: 50px;
    max-width: 9999px;
    color: Black;
    background: #F5F5F5;
}

QListWidget::Item:selected {
    background: lightGray;
    border-left: 5px solid blue;
}
HistoryPanel:hover {
    background: rgb(52, 52, 52);
}